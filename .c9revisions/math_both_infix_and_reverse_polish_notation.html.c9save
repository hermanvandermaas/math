{"ts":1364887377257,"silentsave":true,"restoring":false,"patch":[[{"diffs":[[1,"<!doctype html>\r\n<html>\r\n\r\n<head>\r\n    <meta charset=\"UTF-8\">\r\n    <title>Math</title>\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1, maximum-scale=1\">\r\n    <script src=\"mathparser.js\" type=\"text/javascript\" charset=\"UTF-8\"></script>\r\n    <!-- <script src=\"mathparser-min.js\" type=\"text/javascript\" charset=\"UTF-8\"></script> -->\r\n    <style>\r\n        body {\r\n            background-color: #ddd;\r\n            font-family: 'Helvetica Neue', HelveticaNeue, sans-serif;\r\n            text-shadow: white 0px 1px 1px;\r\n        }\r\n        \r\n        #io, #info {\r\n            width: 95%;\r\n            font-family: monospace;\r\n            font-size: 1.7em;\r\n            border: 1px solid #bbb;\r\n            border-radius: 0.5em;\r\n            padding: 0.2em 0.3em 0.2em 0.4em;\r\n            background-color: white;\r\n        }\r\n    </style>\r\n</head>\r\n\r\n<body>\r\n    <input type=\"text\" id=\"io\" name=\"fname\" value=\"\"><br />\r\n    <input type=\"submit\" id=\"btn\" name=\"bname\" value=\"Go\">\r\n    <p id=\"info\">&nbsp;</p>\r\n    \r\n    <script type=\"text/javascript\" >\r\n        var input = document.getElementById(\"io\");\r\n        var submit = document.getElementById(\"btn\");\r\n        var info = document.getElementById(\"info\");\r\n        \r\n        var mathparser = new Expression('');\r\n        \r\n        submit.addEventListener(\"click\", function() {\r\n            mathparser.Reset();\r\n            var v = input.value;\r\n            mathparser.Expression(v);\r\n            info.innerHTML = mathparser.Evaluate();\r\n            mathparser.Reset();\r\n        }, false);\r\n\r\n    </script>\r\n</body>\r\n\r\n</html>\r\n"]],"start1":0,"start2":0,"length1":0,"length2":1598}]],"length":1598}
